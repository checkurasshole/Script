function switchTab(tabName)
    currentTab = tabName
    for _, tab in ipairs(tabButtons) do
        tabFrames[tab[1]].Visible = (tab[1] == tabName)
        CopyAllButtons[tab[1]].Visible = (tab[1] == tabName)
    end
end

