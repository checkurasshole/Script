local afkFarm = "https://raw.githubusercontent.com/checkurasshole/Script/refs/heads/main/afk-farmbrainrot"
local petFinder = "https://raw.githubusercontent.com/checkurasshole/Script/refs/heads/main/brainrothunterv2"

local gui = Instance.new("ScreenGui")
gui.Parent = game.CoreGui

local frame = Instance.new("Frame")
frame.Parent = gui
frame.Size = UDim2.new(0, 300, 0, 210)
frame.Position = UDim2.new(0.35, 0, 0.35, 0)
frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
frame.BorderSizePixel = 0
frame.Active = true
frame.Draggable = true
Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 8)

local title = Instance.new("TextLabel")
title.Parent = frame
title.Size = UDim2.new(1, -30, 0, 25)
title.Position = UDim2.new(0, 10, 0, 5)
title.BackgroundTransparency = 1
title.Text = "Keyless Brainrot Scripts"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.Font = Enum.Font.GothamSemibold
title.TextSize = 15
title.TextXAlignment = Enum.TextXAlignment.Left

local close = Instance.new("TextButton")
close.Parent = frame
close.Size = UDim2.new(0, 20, 0, 20)
close.Position = UDim2.new(1, -25, 0, 5)
close.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
close.Text = "X"
close.TextColor3 = Color3.fromRGB(255, 255, 255)
close.Font = Enum.Font.GothamBold
close.TextSize = 14
Instance.new("UICorner", close).CornerRadius = UDim.new(0, 4)
close.MouseButton1Click:Connect(function()
	gui:Destroy()
end)

local holder = Instance.new("Frame")
holder.Parent = frame
holder.Size = UDim2.new(1, -20, 1, -45)
holder.Position = UDim2.new(0, 10, 0, 35)
holder.BackgroundTransparency = 1

local layout = Instance.new("UIListLayout", holder)
layout.Padding = UDim.new(0, 6)

local function makeRow(titleText, link)
	local container = Instance.new("Frame")
	container.Size = UDim2.new(1, 0, 0, 35)
	container.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	container.Parent = holder
	Instance.new("UICorner", container).CornerRadius = UDim.new(0, 6)

	local label = Instance.new("TextLabel", container)
	label.Size = UDim2.new(0.45, 0, 1, 0)
	label.Position = UDim2.new(0, 10, 0, 0)
	label.BackgroundTransparency = 1
	label.Text = titleText
	label.TextColor3 = Color3.fromRGB(255, 255, 255)
	label.Font = Enum.Font.Gotham
	label.TextSize = 13
	label.TextXAlignment = Enum.TextXAlignment.Left

	local copy = Instance.new("TextButton", container)
	copy.Size = UDim2.new(0.22, 0, 0.8, 0)
	copy.Position = UDim2.new(0.55, 0, 0.1, 0)
	copy.Text = "Copy"
	copy.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	copy.TextColor3 = Color3.fromRGB(255, 255, 255)
	copy.Font = Enum.Font.Gotham
	copy.TextSize = 13
	Instance.new("UICorner", copy).CornerRadius = UDim.new(0, 4)

	local load = Instance.new("TextButton", container)
	load.Size = UDim2.new(0.22, 0, 0.8, 0)
	load.Position = UDim2.new(0.78, 0, 0.1, 0)
	load.Text = "Load"
	load.BackgroundColor3 = Color3.fromRGB(65, 65, 65)
	load.TextColor3 = Color3.fromRGB(255, 255, 255)
	load.Font = Enum.Font.Gotham
	load.TextSize = 13
	Instance.new("UICorner", load).CornerRadius = UDim.new(0, 4)

	copy.MouseButton1Click:Connect(function()
		setclipboard(('loadstring(game:HttpGet("%s"))()'):format(link))
	end)

	load.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet(link))()
	end)
end

makeRow("AFK Farm Script", afkFarm)
makeRow("Brainrot Pet Finder", petFinder)

-- Buttons for both scripts
local bothCopy = Instance.new("TextButton")
bothCopy.Parent = holder
bothCopy.Size = UDim2.new(1, 0, 0, 35)
bothCopy.Text = "Copy Both Scripts"
bothCopy.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
bothCopy.TextColor3 = Color3.fromRGB(255, 255, 255)
bothCopy.Font = Enum.Font.GothamBold
bothCopy.TextSize = 14
Instance.new("UICorner", bothCopy).CornerRadius = UDim.new(0, 6)
bothCopy.MouseButton1Click:Connect(function()
	setclipboard(([[
loadstring(game:HttpGet("%s"))()
loadstring(game:HttpGet("%s"))()
]]):format(afkFarm, petFinder))
end)

local bothLoad = Instance.new("TextButton")
bothLoad.Parent = holder
bothLoad.Size = UDim2.new(1, 0, 0, 35)
bothLoad.Text = "Load Both Scripts"
bothLoad.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
bothLoad.TextColor3 = Color3.fromRGB(255, 255, 255)
bothLoad.Font = Enum.Font.GothamBold
bothLoad.TextSize = 14
Instance.new("UICorner", bothLoad).CornerRadius = UDim.new(0, 6)
bothLoad.MouseButton1Click:Connect(function()
	loadstring(game:HttpGet(afkFarm))()
	loadstring(game:HttpGet(petFinder))()
end)
