            playerList = playerList .. i .. ". " .. player.DisplayName .. " (@" .. player.Name .. ")\n"
        end
        Rayfield:Notify({Title = "Server List", Content = playerList, Duration = 10})
    end
})

SocialTab:CreateInput({
    Name = "Chat Message",
    PlaceholderText = "Type message to send...",
    RemoveTextAfterFocusLost = false,
    Callback = function(message)
        if message == "" then
            Rayfield:Notify({Title = "Error", Content = "Enter a message!", Duration = 3})
            return
        end
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync(message)
        challengeSystem:updateProgress("chat_master")
        if challengeSystem:awardReward("chat_master") then
            Rayfield:Notify({Title = "Reward", Content = "Received 200 VC for Chat Master!", Duration = 5})
        end
    end
})

local TeleportTab = Window:CreateTab("Teleport", 4483362458)
local popularGames = {
    {Name = "Adopt Me!", PlaceId = 920587237},
    {Name = "Brookhaven", PlaceId = 4924922222},
    {Name = "Blox Fruits", PlaceId = 2753915549},
    {Name = "Murder Mystery 2", PlaceId = 142823291},
    {Name = "Arsenal", PlaceId = 286090429}
}

for _, gameInfo in ipairs(popularGames) do
    TeleportTab:CreateButton({
        Name = "Teleport to " .. gameInfo.Name,
        Callback = function()
            if getLocalStorage("teleportBoostUnlocked", false) then
                TeleportService:Teleport(gameInfo.PlaceId, LocalPlayer)
                challengeSystem:updateProgress("teleporter")
                if challengeSystem:awardReward("teleporter") then
                    Rayfield:Notify({Title = "Reward", Content = "Teleport Boost Unlocked!", Duration = 5})
                end
            else
                Rayfield:Notify({Title = "Locked", Content = "Unlock Teleport Boost via World Traveler challenge!", Duration = 5})
            end
        end
    })
end

TeleportTab:CreateInput({
    Name = "Custom Place ID",
    PlaceholderText = "Enter Place ID...",
    RemoveTextAfterFocusLost = false,
    Callback = function(placeId)
        local id = tonumber(placeId)
        if not id then
            Rayfield:Notify({Title = "Error", Content = "Invalid Place ID!", Duration = 5})
            return
        end
        if getLocalStorage("teleportBoostUnlocked", false) then
            TeleportService:Teleport(id, LocalPlayer)
            challengeSystem:updateProgress("teleporter")
            if challengeSystem:awardReward("teleporter") then
                Rayfield:Notify({Title = "Reward", Content = "Teleport Boost Unlocked!", Duration = 5})
            end
        else
            Rayfield:Notify({Title = "Locked", Content = "Unlock Teleport Boost via World Traveler challenge!", Duration = 5})
        end
    end
})

local ToolsTab = Window:CreateTab("Tools", 4483362458)
ToolsTab:CreateButton({
    Name = "Infinite Jump",
    Callback = function()
        local infiniteJumpEnabled = getLocalStorage("infiniteJumpEnabled", false)
        setLocalStorage("infiniteJumpEnabled", not infiniteJumpEnabled)
        if not infiniteJumpEnabled then
            game:GetService("UserInputService").JumpRequest:Connect(function()
                if LocalPlayer.Character then
                    LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                end
            end)
            Rayfield:Notify({Title = "Tools", Content = "Infinite Jump enabled!", Duration = 5})
        else
            Rayfield:Notify({Title = "Tools", Content = "Infinite Jump disabled!", Duration = 5})
        end
    end
})

ToolsTab:CreateSlider({
    Name = "Walk Speed",
    Range = {16, 100},
    Increment = 1,
    CurrentValue = 16,
    Callback = function(value)
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.WalkSpeed = value
            Rayfield:Notify({Title = "Tools", Content = "Walk Speed set to " .. value, Duration = 3})
        end
    end
})

ToolsTab:CreateSlider({
    Name = "Jump Power",
    Range = {50, 200},
    Increment = 1,
    CurrentValue = 50,
    Callback = function(value)
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.JumpPower = value
            Rayfield:Notify({Title = "Tools", Content = "Jump Power set to " .. value, Duration = 3})
        end
    end
})

ToolsTab:CreateButton({
    Name = "Noclip",
    Callback = function()
        local noclipEnabled = getLocalStorage("noclipEnabled", false)
        setLocalStorage("noclipEnabled", not noclipEnabled)
        if not noclipEnabled then
            RunService.Stepped:Connect(function()
                if getLocalStorage("noclipEnabled", false) and LocalPlayer.Character then
                    for _, part in ipairs(LocalPlayer.Character:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = false
                        end
                    end
                end
            end)
            Rayfield:Notify({Title = "Tools", Content = "Noclip enabled!", Duration = 5})
        else
            if LocalPlayer.Character then
                for _, part in ipairs(LocalPlayer.Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = true
                    end
                end
            end
            Rayfield:Notify({Title = "Tools", Content = "Noclip disabled!", Duration = 5})
        end
    end
})

if subscriptionSystem:isSubscribed(LocalPlayer.UserId) and getLocalStorage("autoUpdateEnabled", true) then
    spawn(function()
        wait(5)
        scriptSystem:loadScript(scriptSystem.vipScript, true)
    end)
end

spawn(function()
    while wait(10) do
        updateAnalytics()
    end
end)

Players.PlayerAdded:Connect(function(player)
    if getLocalStorage("espEnabled", false) and player.Character then
        local highlight = Instance.new("Highlight")
        highlight.FillColor = Color3.fromRGB(255, 0, 0)
        highlight.OutlineColor = Color3.fromRGB(255, 255, 0)
        highlight.Parent = player.Character
    end
end)
