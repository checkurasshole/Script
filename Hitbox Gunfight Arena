end

--- Main Tab Creation
local ResponseTab = Window:CreateTab("ðŸ“ Responses", 4483362458)
local ResponseSection = ResponseTab:CreateSection("âœ‰ï¸ Message System")

-- Variable to store the current response
local currentResponse = ""

-- UI Elements
ResponseTab:CreateInput({
    Name = "ðŸ’­ Your Message",
    PlaceholderText = "Share your thoughts or suggestions here...",
    RemoveTextAfterFocusLost = false,
    Callback = function(text)
        currentResponse = text
    end
})

ResponseTab:CreateButton({
    Name = "ðŸ“¤ Submit Response",
    Info = "Send your message to the development team",
    Interact = "Submit",
    Callback = function()
        local currentTime = tick()
        local timeRemaining = COOLDOWN_SECONDS - (currentTime - lastFeedbackTime)

        if timeRemaining > 0 then
            Rayfield:Notify({
                Title = "â³ Cooldown Active",
                Content = "Please wait " .. formatTime(timeRemaining),
                Duration = 3,
                Image = 4483362458
            })
            return
        end

        if currentResponse == "" then
            Rayfield:Notify({
                Title = "âŒ Error",
                Content = "Please enter a message before submitting",
                Duration = 3,
                Image = 4483362458
            })
            return
        end

        local embedData = {
            title = "ðŸ“¨ New Response",
            description = currentResponse,
            color = 3447003,
            fields = {
                {
                    name = "ðŸ‘¤ User",
                    value = Players.LocalPlayer.DisplayName,
                    inline = true
                },
                {
                    name = "ðŸ†” ID",
                    value = Players.LocalPlayer.UserId,
                    inline = true
                }
            },
            timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ")
        }

        if sendWebhook(embedData) then
            Rayfield:Notify({
                Title = "âœ… Success",
                Content = "Your message has been sent successfully!",
                Duration = 3,
                Image = 4483362458
            })
            lastFeedbackTime = currentTime
            currentResponse = ""
        else
            Rayfield:Notify({
                Title = "âŒ Error",
                Content = "Failed to send message. Please try again later.",
                Duration = 3,
                Image = 4483362458
            })
        end
    end
})

-- Information Labels
ResponseTab:CreateLabel("â° Cooldown: 1 hour and 15 minutes between submissions")
ResponseTab:CreateLabel("ðŸ’¡ Share your suggestions for new features or improvements!")

-- Statistics Section
local StatsSection = ResponseTab:CreateSection("ðŸ“Š Statistics")
local messagesSent = 0

ResponseTab:CreateLabel("Messages Sent Today: " .. messagesSent)

-- Initialize
sendExecutionWebhook()
