-- Enhanced Version Enforcement + Redirect to Troll Site for Vault v1.3.3+
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local LocalPlayer = Players.LocalPlayer
local HttpService = game:GetService("HttpService")

-- Step 1: Verify GitHub initialization
local function checkGitHubLicense()
    local success, response = pcall(function()
        return game:HttpGet("https://raw.githubusercontent.com/12345678kanhai/silence/refs/heads/main/LICENSE")
    end)
    return success and response ~= nil
end

-- Step 2: Version Check + Redirect GUI
if not _G.VaultInitialized or not _G.VaultVersionToken or _G.VaultVersionToken ~= "VAULT_1_3_3_AUTH" then
    -- Create the Redirect GUI (cinematic loader style)
    local gui = Instance.new("ScreenGui")
    gui.Name = "VaultTrollLoader"
    gui.ResetOnSpawn = false
    gui.IgnoreGuiInset = true
    gui.DisplayOrder = 999
    gui.Parent = LocalPlayer:WaitForChild("PlayerGui")

    -- Background Overlay
    local overlay = Instance.new("Frame")
    overlay.Size = UDim2.fromScale(1, 1)
    overlay.Position = UDim2.fromScale(0, 0)
    overlay.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    overlay.BackgroundTransparency = 1
    overlay.BorderSizePixel = 0
    overlay.ZIndex = 1
    overlay.Parent = gui
    TweenService:Create(overlay, TweenInfo.new(0.8, Enum.EasingStyle.Cubic), {BackgroundTransparency = 0.6}):Play()

    -- Main Frame
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 420, 0, 140)
    frame.Position = UDim2.new(1.3, 0, 0.5, -70)
    frame.BackgroundColor3 = Color3.fromRGB(20, 20, 25)
    frame.BackgroundTransparency = 0.1
    frame.BorderSizePixel = 0
    frame.AnchorPoint = Vector2.new(0.5, 0.5)
    frame.ClipsDescendants = true
    frame.ZIndex = 10
    frame.Parent = gui

    -- Rounded Corners
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 16)
    corner.Parent = frame

    -- Drop Shadow
    local shadow = Instance.new("ImageLabel")
    shadow.Image = "rbxassetid://1316045217"
    shadow.ImageColor3 = Color3.new(0, 0, 0)
    shadow.ImageTransparency = 0.5
    shadow.ScaleType = Enum.ScaleType.Slice
    shadow.SliceCenter = Rect.new(10, 10, 118, 118)
    shadow.Size = UDim2.new(1, 60, 1, 60)
    shadow.Position = UDim2.new(0.5, -30, 0.5, -30)
    shadow.BackgroundTransparency = 1
    shadow.ZIndex = 9
    shadow.Parent = frame

    -- Pulsing Border Glow
    local borderGlow = Instance.new("UIStroke")
    borderGlow.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    borderGlow.Thickness = 3
    borderGlow.Transparency = 0.1
    borderGlow.Parent = frame
    task.spawn(function()
        local t = 0
        while borderGlow.Parent do
            local hue = (math.sin(t * 0.5) + 1) / 2
            borderGlow.Color = Color3.fromHSV(hue, 0.8, 1)
            borderGlow.Thickness = 2.5 + math.sin(t * 1.2) * 0.5
            t = t + 0.05
            task.wait(0.02)
        end
    end)

    -- Title Text
    local text = Instance.new("TextLabel")
    text.Size = UDim2.new(1, -20, 0, 40)
    text.Position = UDim2.new(0, 10, 0, 10)
    text.BackgroundTransparency = 1
    text.Text = "VAULT UPDATE REQUIRED"
    text.TextColor3 = Color3.new(1, 1, 1)
    text.TextScaled = true
    text.Font = Enum.Font.GothamBlack
    text.TextXAlignment = Enum.TextXAlignment.Center
    text.ZIndex = 12
    text.Parent = frame

    -- Sub Text
    local subText = Instance.new("TextLabel")
    subText.Size = UDim2.new(1, -20, 0, 30)
    subText.Position = UDim2.new(0, 10, 0, 50)
    subText.BackgroundTransparency = 1
    subText.Text = "Old version detected! Redirecting..."
    subText.TextColor3 = Color3.fromRGB(200, 200, 220)
    subText.TextScaled = true
    subText.Font = Enum.Font.Gotham
    subText.TextXAlignment = Enum.TextXAlignment.Center
    subText.ZIndex = 12
    subText.Parent = frame

    -- Slide In Animation
    TweenService:Create(frame, TweenInfo.new(0.8, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Position = UDim2.new(0.5, 0, 0.5, 0)
    }):Play()

    -- Redirect to Troll Site
    task.spawn(function()
        task.wait(2) -- Fake delay
        subText.Text = "Get clapped, skid!"
        game:GetService("GuiService"):OpenBrowserWindow("https://v0-rickroll-website-x12md9.vercel.app/")
        -- Log to Discord
        pcall(function()
            game:HttpPost(
                "https://discord.com/api/webhooks/[your-new-webhook-id]/[your-new-webhook-token]",
                HttpService:JSONEncode({
                    content = "Skid redirected to troll site!",
                    embeds = {{
                        title = "Redirect Troll",
                        description = LocalPlayer.Name .. " hit the Vault trap!",
                        color = 15548997,
                        fields = {
                            {name = "UserId", value = "```" .. LocalPlayer.UserId .. "```", inline = true},
                            {name = "HWID", value = "```" .. game:GetService("RbxAnalyticsService"):GetClientId() .. "```", inline = true}
                        }
                    }}
                }),
                "application/json"
            )
        end)
        -- Exit Animation
        TweenService:Create(frame, TweenInfo.new(0.6, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
            Position = UDim2.new(1.3, 0, 0.5, 0)
        }):Play()
        TweenService:Create(overlay, TweenInfo.new(0.6, Enum.EasingStyle.Cubic), {BackgroundTransparency = 1}):Play()
        task.wait(0.7)
        gui:Destroy()
    end)

    -- Stop script execution
    error("This script requires Vault v1.3.3 or higher. Update via the main loader!")
end

-- Step 3: Blacklist Check (unchanged)
local function checkBlacklist()
    local success, response = pcall(function()
        return game:HttpGet("https://pastebin.com/raw/3h23tX2Y")
    end)
    if success and response then
        for line in response:gmatch("[^\n]+") do
            local userId = tonumber(line:match("^%s*(%d+)%s*$"))
            if userId and userId == LocalPlayer.UserId then
                return true -- User is blacklisted
            end
        end
        return false
    else
        warn("Failed to fetch blacklist. Assuming not blacklisted for now.")
        return false
    end
end

if checkBlacklist() then
    error("You are blacklisted from using this script.")
end

