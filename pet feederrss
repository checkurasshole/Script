local a,b,c=game:GetService("Players"),game:GetService("ReplicatedStorage"),game:GetService("RunService")local d=a.LocalPlayer;local e=d.Character or d.CharacterAdded:Wait()local f=d:WaitForChild("Backpack")local g,h,i=2,true,0;d.CharacterAdded:Connect(function(j)e=j;f=d:WaitForChild("Backpack")end)local function k()for l,m in ipairs(e:GetChildren())do if m:IsA("Tool")and string.find(m.Name,"%d+%.?%d*kg")then m.Parent=f end end;local n,o=0,3;for l,m in ipairs(f:GetChildren())do if n>=o then break end;if m:IsA("Tool")and string.find(m.Name,"%d+%.?%d*kg")then pcall(function()m.Parent=e;n=n+1 end)task.wait(0.03)end end;return n>0 end;local function p()local q=workspace:FindFirstChild("Farm")if not q then return nil end;for l,r in ipairs(q:GetChildren())do local s=r:FindFirstChild("Important")and r.Important:FindFirstChild("Data")if s and s:FindFirstChild("Owner")and s.Owner.Value==d.Name then return r end end;return nil end;local function t(u,v)local w=u.CFrame:PointToObjectSpace(v)local x=u.Size/2;return math.abs(w.X)<=x.X and math.abs(w.Y)<=x.Y and math.abs(w.Z)<=x.Z end;local function y(z)local A=z:GetAttribute("UUID")or z:GetAttribute("PetUUID")if A then return A end;for l,B in ipairs(z:GetDescendants())do if B.Name=="UUID"and B:IsA("StringValue")then return B.Value end end;return nil end;local function C(D)local E={}local F=workspace:FindFirstChild("PetsPhysical")if not F then return E end;for l,G in ipairs(F:GetChildren())do local H=y(G)if H then for I,J in ipairs(G:GetDescendants())do if J:IsA("BasePart")and t(D,J.Position)then table.insert(E,{UUID=H})break end end end end;return E end;local function K()local L=p()if not L then return 0 end;local M=L:FindFirstChild("PetArea")if not M or not M:IsA("BasePart")then return 0 end;local N=C(M)local O=0;for l,P in ipairs(N)do local Q=k()if Q then task.wait(0.05)pcall(function()local R=b:FindFirstChild("GameEvents")if R then local S=R:FindFirstChild("ActivePetService")if S then S:FireServer("Feed",P.UUID)O=O+1 end end end)task.wait(0.1)end end;return O end;task.spawn(function()while true do if h then i=K()end;task.wait(g)end end)a.PlayerRemoving:Connect(function(T)if T==d then end end)return{GetData=function()return{FedCount=i}end,ManualFeed=function()i=K()return i end,SetAutoFeed=function(U)h=U end,SetFeedDelay=function(V)g=math.max(V,0.1)end}
